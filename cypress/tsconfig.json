{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "allowJs": true,
    "outDir": "../../dist/out-tsc",
    "module": "commonjs",
    "types": ["cypress", "node"],
    "sourceMap": false,
    "target": "es5",
    "lib": ["es5", "dom"],
    "jsx": "preserve"

  },  
  "include": [
    "./src",
    "cypress.d.ts",
    "**/*.ts",
    "**/*.js",
    "../**/*.cy.ts",
    "../**/*.cy.tsx",
    "../**/*.cy.js",
    "../**/*.cy.jsx",
    "../../**/*.d.ts"
  ]
}
